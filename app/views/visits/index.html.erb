<h1>Listing visits</h1>
<div class = "row">
  <div class = "large-8 columns">
    <table>
      <thead>
        <tr>
          <th>Venue</th>
          <th>City</th>
          <th>Date</th>
          <th>Price</th>
          
          <th>Total Score</th>
          <th></th>
          <th></th>
          <th></th>
       
        </tr>
      </thead>

      <tbody>
        <% @visits.each do |visit| %>
          <tr>
            <td><%= visit.venue %></td>
            <td><%= visit.city %></td>
            <td><%= visit.date %></td>
            <td><%= visit.price %></td>
     
            <td><%= visit.total_score %> </td>
            <td><%= link_to 'Details', visit %></td>
            <td><%= link_to 'Edit', edit_visit_path(visit) %></td>
            <td><%= link_to 'Destroy', visit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
<br>

<%= link_to 'New Visit', new_visit_path, :class => "button radius"%>

